{
	"name": "ooc",
	"version": "0.1.0",
	"publisher": "magic-lang",
	"engines": {
		"vscode": "*"
	},
	"contributes": {
		"languages": [
			{
				"id": "ooc",
				"extensions": [
					".ooc"
				],
				"aliases": [
					"ooc"
				],
				"configuration": "./ooc.configuration.json"
			}
		],
		"grammars": [
			{
				"language": "ooc",
				"scopeName": "source.ooc",
				"path": "./syntaxes/ooc.json"
			}
		],
		"snippets": [
			{
				"language": "ooc",
				"scopeName": "source.ooc",
				"path": "./snippets/ooc.json"
			}
		]
	}
}
